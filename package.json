{
  "name": "sharp-gpu",
  "version": "0.1.0",
  "description": "GPU-accelerated image processing library for the web",
  "packageManager": "bun@1.2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "dev:example": "cd example && bun install && bun run dev",
    "build:example": "cd example && bun install && bun run build"
  },
  "keywords": [
    "image",
    "processing",
    "gpu",
    "webgl",
    "regl",
    "blur",
    "filter",
    "graphics",
    "canvas",
    "image-manipulation"
  ],
  "author": "Rodrigo",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "regl": "^2.1.1"
  }
}
